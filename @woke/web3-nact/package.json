    "@woke/lib": "0.4.0",
    "nact": "^7.2.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "minimist": "^1.2.0",
    "mocha": "^6.2.0"
  }
