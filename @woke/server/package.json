{
  "name": "@woke/server",
  "version": "0.4.0",
  "description": "Wallet backend: account creation and funding.",
  "private": true,
  "scripts": {
    "start": "DEBUG=server:* node ./bin/www",
    "start:production": "NODE_ENV=production ETH_ENV=production WOKE_ROLE=funder DEBUG=server:* node ./bin/www",
    "db": "docker-compose -f ../../server.docker-compose.local.yml up -d --build db",
    "down": "docker-compose -f ../../server.docker-compose.local.yml down",
    "reset": "docker-compose -f ../../server.docker-compose.local.yml down -v",
    "dev": "NODE_ENV=development ETH_ENV=development DEBUG=server:* node ./bin/www",
    "ropsten": "NODE_ENV=development ETH_ENV=ropsten DEBUG=server:* node ./bin/www",
    "rinkeby": "NODE_ENV=development ETH_ENV=rinkeby DEBUG=server:* node ./bin/www",
    "goerli": "NODE_ENV=development ETH_ENV=goerli DEBUG=server:* node ./bin/www"
  },
  "dependencies": {
    "@woke/lib": "0.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nconf": "^0.10.0",
    "pg": "^7.11.0",
    "sequelize": "^5.8.6",
    "umzug": "^2.2.0",
    "web3": "^1.2.4",
    "debug": ">=4.1"
  },
  "devDependencies": {
    "standard": "^12.0.1"
  }
}
