{
  "name": "@woke/oracle",
  "version": "0.1.0",
  "description": "Tiny oracle for Twitter API to avoid waiting for Oraclize",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-debug": "DEBUG=oracle:* ./node_modules/.bin/mocha",
    "start": "NODE_ENV=production ETH_ENV=production WOKE_ROLE=oracle DEBUG=oracle* node src/index.js",
    "start-docker": "DEBUG=oracle* node src/index.js",
    "dev": "NODE_ENV=development ETH_ENV=development DEBUG=*,twitter node src/index.js",
    "ropsten": "NODE_ENV=development ETH_ENV=ropsten DEBUG=oracle*,twitter node src/index.js",
    "rinkeby": "NODE_ENV=development ETH_ENV=rinkeby DEBUG=oracle*,twitter node src/index.js",
    "goerli": "NODE_ENV=development ETH_ENV=goerli DEBUG=oracle*,twitter node src/index.js",
    "dev:with_client": "DEBUG=oracle:* node src/lib/client.js",
    "tweet": "DEBUG=twitter* node ../lib/twitter.js",
    "cli": "node src/cli.js",
    "cli:verbose": "DEBUG=* node src/cli.js"
  },
  "author": "Jackson Virgo",
  "license": "ISC",
  "dependencies": {
    "@woke/actors": "0.1.0",
    "@woke/contracts": "0.4.0",
    "@woke/lib": "0.4.0",
    "@woke/wact": "0.1.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "web3": "^1.2.9",
    "minimist": "^1.2.0",
    "mocha": "^6.2.0"
  }
}
